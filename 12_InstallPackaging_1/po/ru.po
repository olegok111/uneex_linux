#. A bunch of seemingly unused strings wrapped in N_().
#. This is a hack to make sure xgettext picks up default help strings.
#: ../src/guess.c:36
msgid "Give this help list"
msgstr "Показать эту справку"

#: ../src/guess.c:37
msgid "Give a short usage message"
msgstr "Показать короткую справку по использованию"

#: ../src/guess.c:38
msgid "Print program version"
msgstr "Показать номер версии программы"

#: ../src/guess.c:39
msgid "Usage:"
msgstr "Использование:"

#: ../src/guess.c:40
msgid " [OPTION...]"
msgstr " [ПАРАМЕТР...]"

#: ../src/guess.c:41
#, c-format
msgid "Report bugs to %s.\n"
msgstr "Об ошибках сообщать по адресу %s.\n"

#: ../src/guess.c:50
msgid "A game where the computer guesses the number picked by the player."
msgstr "Игра, где компьютер угадывает число, загаданное игроком."

#: ../src/guess.c:54
msgid "Use roman numerals"
msgstr "Использовать римские цифры"

#: ../src/guess.c:101
#, c-format
msgid "Think of a number no less than I and no more than %.*s.\n"
msgstr "Загадай число не меньше I и не больше %.*s.\n"

#: ../src/guess.c:106
#, c-format
msgid "Think of a number no less than 1 and no more than %u.\n"
msgstr "Загадай число не меньше 1 и не больше %u.\n"

#: ../src/guess.c:117
#, c-format
msgid "Is the number more than %.*s?"
msgstr "Число больше %.*s?"

#: ../src/guess.c:122
#, c-format
msgid "Is the number more than %u?"
msgstr "Число больше %u?"

#: ../src/guess.c:127
msgid "y\n"
msgstr "д\n"

#: ../src/guess.c:135
#, c-format
msgid "The number must be %.*s\n"
msgstr "Должно быть, число равно %.*s\n"

#: ../src/guess.c:140
#, c-format
msgid "The number must be %u\n"
msgstr "Должно быть, число равно %u\n"
